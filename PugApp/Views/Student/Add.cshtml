@model PugApp.Models.Student
@*add students*@

@{
	ViewBag.Title = "Add";
}

<h2>Add</h2>

@using (Html.BeginForm())
{
	@Html.HiddenFor(s => s.ID);

	@Html.LabelFor(s => s.LastName, "Last Name");
	@Html.TextBoxFor(s => s.LastName);

	@Html.LabelFor(s => s.LastName, "First and Middle Names");
	@Html.TextBoxFor(s => s.FirstMidName);

	<button type="submit">Add</button>
}